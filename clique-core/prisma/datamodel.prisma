enum EventType {
  HAPPY_HOUR
}

enum EventStatus {
  COMPLETED
  PLANNED
  INCOMPLETE
  CANCELLED
}

type Cliq {
  id: ID! @unique
  participants: [ID!]
  pendingParticipants: [ID!]
}

type Event {
  id: ID! @unique
  locationId: ID
  cliq: Cliq
  createdAt: DateTime!
  updatedAt: DateTime!
  eventTime: DateTime
  type: EventType!
  status: EventStatus!
}
